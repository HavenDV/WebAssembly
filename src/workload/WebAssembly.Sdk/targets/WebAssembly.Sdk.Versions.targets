<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <_DefaultTargetPlatformVersion>7.0</_DefaultTargetPlatformVersion>
  </PropertyGroup>

  <PropertyGroup>
    <TargetPlatformSupported>true</TargetPlatformSupported>
    <TargetPlatformVersion Condition=" '$(TargetPlatformVersion)' == '' ">$(_DefaultTargetPlatformVersion)</TargetPlatformVersion>
    <_TargetFramework Condition="$(TargetFramework.EndsWith('webassembly'))">$(TargetFramework)$(_DefaultTargetPlatformVersion)</_TargetFramework>
    <_TargetFramework Condition="!$(TargetFramework.EndsWith('webassembly'))">$(TargetFramework)</_TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <SupportedPlatform Include="webassembly" />
  </ItemGroup>

  <ItemGroup>
    <SupportedAPILevel Include="v4.0" MappedAPIVersion="4.0"/>
    <SupportedAPILevel Include="v5.0" MappedAPIVersion="5.0"/>
    <SupportedAPILevel Include="v6.0" MappedAPIVersion="5.0"/>
    <SupportedAPILevel Include="v7.0" MappedAPIVersion="5.5"/>
    <SupportedAPILevel Include="v8.0" MappedAPIVersion="6.0"/>
    <SupportedAPILevel Include="v9.0" MappedAPIVersion="6.5"/>
    <SupportedAPILevel Include="v10.0" MappedAPIVersion="7.0"/>
    <SupportedAPILevel Include="v11.0" MappedAPIVersion="8.0"/>
  </ItemGroup>

  <ItemGroup>
    <WebAssemblySdkSupportedTargetPlatformVersion Include="6.5" />
    <WebAssemblySdkSupportedTargetPlatformVersion Include="7.0" />
    <WebAssemblySdkSupportedTargetPlatformVersion Include="8.0" />
    <WebAssemblySdkSupportedTargetPlatformVersion Include="1.0" />
  </ItemGroup>

  <ItemGroup>
    <SdkSupportedTargetPlatformVersion Include="@(WebAssemblySdkSupportedTargetPlatformVersion)" />
  </ItemGroup>

  <ItemGroup>
    <KnownFrameworkReference
      Include="WebAssembly"
      TargetFramework="$(_TargetFramework)"
      TargetingPackName="WebAssembly.Ref"
      TargetingPackVersion="**FromWorkload**"
      Profile="WebAssembly"
      />
  </ItemGroup>

</Project>
