{
  "$schema": "http://json.schemastore.org/template",
  "author": "HavenDV",
  "classifications": [ "WebAssembly" ],
  "identity": "WebAssembly.WebAssemblyApp",
  "name": "WebAssembly .NET Application",
  "description": "A project for creating an WebAssembly .NET application",
  "shortName": "webassembly",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "WebAssemblyApp1",
  "preferNameDirectory": true,
  "primaryOutputs": [
    { "path": "WebAssemblyApp1.csproj" }
  ],
  "symbols": {
    "packageName": {
      "type": "parameter",
      "description": "Overrides the package name in the webassembly-manifest.xml",
      "datatype": "string",
      "replaces": "com.companyname.WebAssemblyApp1"
    }
  },
  "defaultName": "WebAssemblyApp1"
}